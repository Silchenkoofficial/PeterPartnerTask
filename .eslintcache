 }) {\n  const [newBalance, setNewBalance] = useState(0);\n\n  const changeNewBalance = value => {\n    setNewBalance(value);\n  };\n\n  return (\n    <div className=\"user-card\">\n      <div className=\"user-card__section\">\n        <CardType type={cardInfo.type} />\n        <div className=\"user-card__number\">{cardInfo.card_number}</div>\n      </div>\n      <div className=\"user-card__section\">\n        <div className=\"user-card__name\">\n          <UserIcon /> <p>{cardInfo.cardholder_name}</p>\n        </div>\n        <div className=\"user-card__valid-thru\">\n          <p className=\"text\">VALID THRU</p>\n          <p className=\"value\">{cardInfo.valid}</p>\n        </div>\n      </div>\n      <div className=\"user-card__section\">\n        <div className=\"user-card__currency\">\n          <p>{activeCurrency === \"GBP\" && \"£\"}</p>\n          <p>{activeCurrency === \"EUR\" && \"€\"}</p>\n          <p>{activeCurrency === \"RUB\" && \"₽\"}</p>\n          <Valute\n            balance={cardInfo.balance}\n            currency={activeCurrency}\n          />\n        </div>\n        <div className=\"user-card__balance\">\n          <p className=\"text\">Your balance</p>\n          <p className=\"value\">\n            $\n            {cardInfo.balance &&\n              cardInfo.balance\n                .toString()\n                .replace(/(\\d)(?=(\\d\\d\\d)+([^\\d]|$))/g, \"$1 \")}\n          </p>\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default Card;\n","/Users/vasilyevskaya/Desktop/projects/PeterPartnerTask/src/components/SubTitle/SubTitle.jsx",[],"/Users/vasilyevskaya/Desktop/projects/PeterPartnerTask/src/pages/Home/components/History/History.jsx",[],"/Users/vasilyevskaya/Desktop/projects/PeterPartnerTask/src/components/HistoryItem/HistoryItem.jsx",[],"/Users/vasilyevskaya/Desktop/projects/PeterPartnerTask/src/components/Valute/Valute.jsx",["112"],"import React, {useState, useEffect} from 'react';\nimport axios from 'axios';\n\nfuncti